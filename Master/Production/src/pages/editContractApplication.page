  <script type="text/javascript">
		$j = jQuery.noConflict();

		$j('#jsi_tabList li').on('click', function(){
			// クリックされたインデックス
			var index = $j('#jsi_tabList li').index(this);
			// コンテンツを一度すべて非表示
			$j('#jsi_pdfList li').css('display', 'none');
			// クリックされたタブと同じ順番のコンテンツを表示
			$j('#jsi_pdfList li').eq(index).css('display', 'block');
			// タブのセレクト(current)を付け直し
			$j('#jsi_tabList li').removeClass('current');
			$j(this).addClass('current');
		});

		$j(document).ready(function($) {
			// コンテンツを一度すべて非表示し、一つ目の要素だけ表示
			$j('#jsi_pdfList li').css('display', 'none');
			$j('#jsi_pdfList li').eq(0).css('display', 'block');

			// タブのセレクト(current)を付け直し
			$j('#jsi_tabList li').removeClass('current');
			$j('#jsi_tabList li').eq(0).addClass('current');

		});
		
		function update() {
			var result = saveProductAll();
			alert(result);
			
			return true;
		}
		
		function cancel() {
			var infoId = "{!infoId}";
			if (infoId.length > 0) {
				window.location.href = "/apex/viewContractApplication?Id=" + infoId;
			} else {
				// 戻す場所がわからない場合はトップ画面に戻す。
				window.location.href = "/";
			}
			return false;
		}
		
		function avoidDoubleSubmitUpdate() {
      document.getElementById('btn_overlay').style.display = 'block';
      setTimeout(function() {
        document.getElementById('btn_overlay').style.display = 'none';
      }, 10000);
    }

		function avoidDoubleSubmitDelete() {
      document.getElementById('btn_overlay').style.display = 'block';
      setTimeout(function() {
        document.getElementById('btn_overlay').style.display = 'none';
      }, 10000);
    }
  </script>
