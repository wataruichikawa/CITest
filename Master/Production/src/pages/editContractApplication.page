	<style type="text/css">
		.rep-tabList {
			list-style: none;
		    position: relative;
		    z-index: 10;
		    padding: 0 10px;
		}
		.rep-tabList li {
			float: left;
			margin-right: 2px;
		    margin-left: 0px !important;
		}
		.rep-tabList a {
		    display: block;
		    width: 135px;
		    min-height: 30px;
		    text-align: center;
		    border-width: 1px 1px 0 1px;
		    border-style: solid;
		    padding: 8px 0px;
		    border-radius: 4px 4px 0 0;
		    margin-top: 3px;
		    text-decoration: none !important;
		    font-weight: bold;
		    border-color: #CFCFCF;
		    color: #333333 !important;
		}
		.rep-tabList a:hover {
		    border-color: #35B4E9;
		    background-color: #BADCE7;
		    color: #35B4E9 !important;
		}
		.rep-tabList .current a {
		    border-color: #35B4E9;
		    background-color: #D5EBF2;
		    color: #35B4E9 !important;
		}
		.pdf-tabList {
			list-style: none;
		    position: relative;
		    z-index: 10;
		    padding: 0 10px;
		}
		.pdf-tabList li {
			float: left;
			margin-right: 2px;
		    margin-left: 0px !important;
		}

    .inputLookup {
      width: 320px;
    }

    .outputLookup {
      border-style: solid;
      border-color: #DDB929;
      border-width: 2px 0px 2px;
      margin-top: 4px;
      margin-bottom: 4px;
      background-color: #FFFFFF;
    }

    .outputLookupMstProduct {
